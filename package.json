{
  "name": "lovel-up",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "client": "cd client && npm start",
    "server": "node app.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "knex": "knex migrate:rollback && knex migrate:latest && knex seed:run"
  },
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
